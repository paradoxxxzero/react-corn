    "lerna": "^3.20.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "prismjs": "^1.20.0",
    "react-simple-code-editor": "^0.11.0",
    "rollup": "^2.10.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
